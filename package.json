{
  "name": "joo-browser-detect",
  "version": "0.0.3",
  "description": "This is a package that attempts to be a tool for helping people detecting browser, check for support/valid browser and fire when a browser error happens",
  "main": "dist/joo.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development webpack --mode=production --progress --colors --watch",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js",
    "predeploy": "webpack --optimize-minimize",
    "deploy": "gh-pages -d deploy"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "eslint": "4.18.2",
    "eslint-loader": "^2.1.2",
    "gh-pages": "^1.2.0",
    "mocha": "^8.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shayanypn/joo-browser-detect"
  },
  "keywords": [
    "browser",
    "browser detect",
    "detect",
    "useragent"
  ],
  "author": "Shayan Yazdanpanah <shayanypn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shayanypn/joo-browser-detect/issues"
  },
  "homepage": "https://github.com/shayanypn/joo-browser-detect",
  "dependencies": {
    "false": "^0.0.4"
  }
}
