{
  "name": "joo-browser-detect",
  "version": "0.0.2",
  "description": "This is a package that attempts to be a tool for helping people detecting browser, check for support/valid browser and fire when a browser error happens",
  "main": "dist/joo.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "predeploy": "webpack --optimize-minimize",
    "deploy": "gh-pages -d deploy"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "gh-pages": "^1.2.0",
    "mocha": "^5.2.0",
    "webpack": "3.1.0",
    "yargs": "7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shayanypn/joo-browser-detect"
  },
  "keywords": [
    "browser",
    "browser detect",
    "detect",
    "useragent"
  ],
  "author": "Shayan Yazdanpanah <shayanypn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shayanypn/joo-browser-detect/issues"
  },
  "homepage": "https://github.com/shayanypn/joo-browser-detect",
  "dependencies": {
    "false": "^0.0.4"
  }
}
